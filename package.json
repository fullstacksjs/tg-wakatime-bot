{
  "name": "wakatime-bot",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc && shx cp -r src/api/web/ dist/api/",
    "dev": "node --watch --loader tsm ./src/main.ts",
    "api": "node dist/main.js api",
    "bot": "node dist/main.js bot",
    "prepare": "husky install",
    "lint": "npm-run-all -p lint:*",
    "lint:style": "stylelint \"src/api/web/css/*.css\"",
    "lint:es": "eslint . --fix"
  },
  "dependencies": {
    "@fullstacksjs/eslint-config": "9.8.0",
    "@fullstacksjs/toolbox": "3.0.2",
    "@fullstacksjs/tsconfig": "1.1.0",
    "@grammyjs/ratelimiter": "1.1.6",
    "@types/cron": "2.0.1",
    "@types/express": "4.17.17",
    "@types/node": "20.1.6",
    "awilix": "8.0.1",
    "axios": "1.4.0",
    "cron": "2.3.0",
    "deepmerge-ts": "5.1.0",
    "dotenv": "16.0.3",
    "dotenv-cli": "7.2.1",
    "ejs": "3.1.9",
    "eslint": "8.44.0",
    "express": "4.18.2",
    "grammy": "1.12.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "lowdb": "6.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.8",
    "puppeteer": "20.2.1",
    "shx": "0.3.4",
    "stylelint": "15.6.1",
    "stylelint-config-standard": "33.0.0",
    "ts-dedent": "2.2.0",
    "tsm": "2.3.0",
    "typescript": "5.1.6"
  },
  "volta": {
    "node": "18.16.0",
    "npm": "9.6.7"
  }
}
